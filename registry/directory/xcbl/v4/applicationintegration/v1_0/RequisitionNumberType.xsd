    <xsd:complexType name="RequisitionNumberType">
        <xsd:annotation>
            <xsd:documentation>contains the unique number and name assigned to identify a requistion.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ReqNumber" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>is a unique number assigned to the <!--code-->Requisition<!--/code--> by the system generating the requisition transaction.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ReqName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>is a descriptive name assigned to the <!--code-->Requisition<!--/code--> by the user generating the requisition transaction.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="RevisionNumber" type="core:ReferenceType">
                <xsd:annotation>
                    <xsd:documentation>contains the requisition revision number along with an optional revision date element.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
