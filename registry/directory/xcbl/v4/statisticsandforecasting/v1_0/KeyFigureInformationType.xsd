    <xsd:complexType name="KeyFigureInformationType">
        <xsd:annotation>
            <xsd:documentation>is used to identify the name, type, and value of a key figure
        used in a time series with respect to the source or target systems.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="SourceKeyFigure" type="KeyFigureType">
                <xsd:annotation>
                    <xsd:documentation>is the key figure data with respect to the source system.
        </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TargetKeyFigure" type="KeyFigureType">
                <xsd:annotation>
                    <xsd:documentation>is the key figure data with respect to the target system.
        </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
