    <xsd:complexType name="CharacteristicOtherType">
        <xsd:annotation>
            <xsd:documentation>is a generic configurable characteristic element that can be used
        to describe various characteristics of a time series. This element is given
        with respect to the source and target systems. </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="SourceCharacteristicsOther" type="CharacteristicAttributeType">
                <xsd:annotation>
                    <xsd:documentation>is other characteristics described with respect to the source
        system.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TargetCharacteristicsOther" type="CharacteristicAttributeType">
                <xsd:annotation>
                    <xsd:documentation>is other characteristics described with respect to the target
        system.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
