    <xsd:complexType name="KeyFigureType">
        <xsd:annotation>
            <xsd:documentation>is the key figure data used to identify the name, type, and value
        of a key figure used in a time series.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="KeyFigureID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>is the key that uniquely identifies a key figure via its
        technical name with respect to the <!--code-->TimeSeriesSystemType<!--/code--> given at the
        header. Eg: APO_FORECAST</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="KeyFigureName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>contains the predefined name used for the key figure. Eg: sales forecast</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="KeyFigureDescription" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>contains the alternate name used to rename the given key
        figure. Eg: my sales forecast</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
