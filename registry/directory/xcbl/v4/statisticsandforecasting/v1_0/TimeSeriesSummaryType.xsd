    <xsd:complexType name="TimeSeriesSummaryType">
        <xsd:annotation>
            <xsd:documentation>contains all information relevant to the summary of the time
        series document.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="TotalTimeSeriesData" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>is the total number of time series items given in the
        document.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TotalCharacteristicCombinations" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>is the total number of characteristic combinations given in the
        document.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="TotalKeyFigures" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>is the total number of key figures.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
