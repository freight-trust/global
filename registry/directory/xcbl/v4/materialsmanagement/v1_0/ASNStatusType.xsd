    <xsd:complexType name="ASNStatusType">
        <xsd:annotation>
            <xsd:documentation>identifies the status of the Order/Orders that the ASN is referencing and transmitting. </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ASNStatusCoded" type="ASNStatusCodeType">
                <xsd:annotation>
                    <xsd:documentation>indicates the status of the Order/Orders covered by the ASN.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ASNStatusCodedOther" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>is used to provide a non-standard <!--code-->ASNStatusCode.<!--/code--> This element is mandatory
     if the value of <!--code-->ASNStatusCoded<!--/code--> is 'Other'. These codes should not contain white space
     unless absolutely necessary.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
