    <xsd:simpleType name="ASNStatusCodeType">
        <xsd:annotation>
            <xsd:documentation>This code identifies the status of the orders covered by the ASN message.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:NMTOKEN">
            <xsd:enumeration value="Other">
                <xsd:annotation>
                    <xsd:documentation>Custom Code</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="CompleteOrder">
                <xsd:annotation>
                    <xsd:documentation>The ASN covers one complete order</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PartialOrder">
                <xsd:annotation>
                    <xsd:documentation>The ASN covers part of one order</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ManyCompleteOrders">
                <xsd:annotation>
                    <xsd:documentation>The ASN covers more than one complete order</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ManyPartialOrders">
                <xsd:annotation>
                    <xsd:documentation>The ASN covers more than one partial order</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="MixedOrder">
                <xsd:annotation>
                    <xsd:documentation>The ASN covers a mixture of partial or complete orders</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>
