    <xsd:complexType name="InventoryReportDatesType">
        <xsd:annotation>
            <xsd:documentation>indicates the pertinent dates for the InventoryReport. These dates are
		global and apply to the entire inventory report, except where overridden by dates provided at the line item or package level
 </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="StartDate" type="xsd:dateTime">
                <xsd:annotation>
                    <xsd:documentation>indicates the date and time for which the inventory report information begins.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EndDate" type="xsd:dateTime">
                <xsd:annotation>
                    <xsd:documentation>indicates the date and time for which the inventory report information ends.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ListOfDateCoded" type="core:ListOfDateCodedType">
                <xsd:annotation>
                    <xsd:documentation>is used to communicate any other dates not explicitly stated by another element.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
