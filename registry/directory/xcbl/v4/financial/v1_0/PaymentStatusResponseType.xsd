    <xsd:complexType name="PaymentStatusResponseType">
        <xsd:annotation>
            <xsd:documentation>. The payment status response document enables the financial institution to send the information on the status of a payment to the payer.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="PaymentStatusResponseHeader" type="PaymentStatusResponseHeaderType">
                <xsd:annotation>
                    <xsd:documentation>contains the header information of the <!--code-->PaymentStatusResponse<!--/code-->.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ListOfPaymentStatusResponseDetail" type="ListOfPaymentStatusResponseDetailType">
                <xsd:annotation>
                    <xsd:documentation>contains a list of <!--code-->PaymentStatusResponse<!--/code--> details.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PaymentStatusResponseSummary" type="PaymentStatusRequestSummaryType">
                <xsd:annotation>
                    <xsd:documentation>contains the summary information of the <!--code-->PaymentStatusResponse<!--/code--> document.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
