    <xsd:complexType name="CurrencyAvailabilityCheckListType">
        <xsd:annotation>
            <xsd:documentation>indicates multiple types of payment services available for the exchange currency.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="FundsTransferIndicator" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>indicates whether payment services for funds transfer is available.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="RemoteCheckIndicator" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>indicates whether payment services for remote checks is available.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="OnsiteCheckIndicator" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>indicates whether payment services for onsite checks is available.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ACHIndicator" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>indicates whether payment services for ACH is available.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="RemitCurrencyIndicator" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>indicates whether payment services for remit currency is available.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="OtherPaymentServiceNote" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>contains a general note field to capture details pertaining to other payment services provided by the financial institution.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
