    <xsd:simpleType name="RemittanceAdviceStatusCodeType">
        <xsd:annotation>
            <xsd:documentation>Identifies the status of the remittance advice in terms of the payment stage.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:NMTOKEN">
            <xsd:enumeration value="Other">
                <xsd:annotation>
                    <xsd:documentation>Custom Code</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Approved">
                <xsd:annotation>
                    <xsd:documentation>Payment approved</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PaymentReleased">
                <xsd:annotation>
                    <xsd:documentation>Payment released to Financial Institution</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Paid">
                <xsd:annotation>
                    <xsd:documentation>Payment confirmed by Financial Institution</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
